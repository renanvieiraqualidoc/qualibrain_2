    function get_percentage($totalp, $numberp)
    {
      if ( $totalp > 0 ) {
       return round($numberp / ($totalp / 100),2);
      } else {
        return 0;
      }
    }



// Função de porcentagem: N é X% de N
function porcentagem_nx ( $parcial, $total ) {
    return ( $parcial * 100 ) / $total;

}












